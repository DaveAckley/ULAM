#>Foo.ulam
  ulam 2;
use System;
use Soo;
element Foo : Soo {
  Bool(7) sp;
  Int a = 73;
  Void setus(Unsigned(3) x)
  {
    us = x;
  }

  Int test() {
    System s;
    setus(5u); //Unsigned(3)
    Coo cc = (Coo) self; //also works
    s.print(cc.us); //not default 2u
    s.assert(cc.us == 5);

    cc.setus(7u);
    if(cc.cb)
      {
	s.print(us); //5u
	s.assert(us == 5);
	s.print(cc.us); //7u
	s.assert(cc.us == 7);
	return us + cc.us; //exit status 12
      }
    s.assert(false);
    return 0;
  }
}

